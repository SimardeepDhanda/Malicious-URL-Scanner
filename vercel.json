{
  "version": 2,
  "installCommand": "cd backend && npm install",
  "buildCommand": "",
  "functions": {
    "api/index.ts": {
      "includeFiles": "backend/**",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/index.ts"
    },
    {
      "source": "/scan",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}
